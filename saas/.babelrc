{
    "presets": ["es2015", "stage-2"],
    "plugins": [
        "transform-runtime",
        "transform-vue-jsx",
        "syntax-jsx",
        "syntax-dynamic-import"
    ],
    "comments": false,
    "env": {
        "production": {
            "plugins": [
                "transform-remove-console",
                "transform-remove-debugger",
                "syntax-dynamic-import"
            ]
        }
    }
}

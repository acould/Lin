<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="VersionDetailMapper">
	<!--表名 -->
	<sql id="tableName">
		tb_versiondetail
	</sql>

	<!-- 字段 -->
	<sql id="Field">
		id,
		type,
		contenet,
		version_id
	</sql>

	<!-- 字段值 -->
	<sql id="FieldValue">
		#{id},
		#{type},
		#{contenet},
		#{version_id}
	</sql>
</mapper>